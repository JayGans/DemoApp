<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.ganesh.demoapp.ui.home.HomeFragment"
        android:label="Dashboard"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_invoice"
        android:name="com.ganesh.demoapp.ui.dashboard.DashboardFragment"
        android:label="Invoices"
        tools:layout="@layout/fragment_dashboard" />
    <fragment
        android:id="@+id/navigation_collect"
        android:name="com.ganesh.demoapp.ui.notifications.NotificationsFragment"
        android:label="Collect"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_cust"
        android:name="com.ganesh.demoapp.ui.notifications.NotificationsFragment"
        android:label="Customer"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_more"
        android:name="com.ganesh.demoapp.ui.notifications.NotificationsFragment"
        android:label="More"
        tools:layout="@layout/fragment_notifications" />
